rule "10 - Set random wifi variations"
when
    System started or
    Time cron "/20 * * * * ?"
then
    postUpdate(Wifi_Level, (Math :: random * 4.0).intValue)

end

rule "6 - Initialize Location"
when
    Item Wifi_Level changed
then
    DemoLocation.postUpdate(new PointType ("52.5200066,13.4049540"))

end